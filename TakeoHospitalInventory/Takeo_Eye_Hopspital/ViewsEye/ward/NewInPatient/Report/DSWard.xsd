            <MainSource>
              <DbSource ConnectionRef="TakeoDBConnectionString (MySettings)" DbObjectName="TakeoDB.dbo.TblNewInPatient" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        T1.Diagnosis, COUNT(T2.Male) AS M, COUNT(T2.Female) AS F, '0-4' AS AgeRange
FROM            TblPatients AS T2 INNER JOIN
                         TblNewInPatient AS T1 ON T2.PatientNo = T1.PatientNo
WHERE        (T2.Age BETWEEN 0 AND 4) AND (CAST(CONVERT(VARCHAR(10), T1.CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), 
                         CAST(@DateFrom AS DATETIME), 1) AS Datetime) AND CAST(CONVERT(VARCHAR(10), CAST(@DateTo AS DATETIME), 1) AS Datetime)) AND (T1.DeleteOption = 'False')
GROUP BY T1.Diagnosis
UNION
SELECT        T1.Diagnosis, COUNT(T2.Male) AS M, COUNT(T2.Female) AS F, '5-14' AS AgeRange
FROM            TblPatients AS T2 INNER JOIN
                         TblNewInPatient AS T1 ON T1.PatientNo = T2.PatientNo
WHERE        (T2.Age BETWEEN 5 AND 14) AND (CAST(CONVERT(VARCHAR(10), T1.CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), 
                         CAST(@DateFrom AS DATETIME), 1) AS Datetime) AND CAST(CONVERT(VARCHAR(10), CAST(@DateTo AS DATETIME), 1) AS Datetime)) AND (T1.DeleteOption = 'False')
GROUP BY T1.Diagnosis
UNION
SELECT        T1.Diagnosis, COUNT(T2.Male) AS M, COUNT(T2.Female) AS F, '15-49' AS AgeRange
FROM            TblPatients AS T2 INNER JOIN
                         TblNewInPatient AS T1 ON T1.PatientNo = T2.PatientNo
WHERE        (T2.Age BETWEEN 15 AND 49) AND (CAST(CONVERT(VARCHAR(10), T1.CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), 
                         CAST(@DateFrom AS DATETIME), 1) AS Datetime) AND CAST(CONVERT(VARCHAR(10), CAST(@DateTo AS DATETIME), 1) AS Datetime)) AND (T1.DeleteOption = 'False')
GROUP BY T1.Diagnosis
UNION
SELECT        T1.Diagnosis, COUNT(T2.Male) AS M, COUNT(T2.Female) AS F, '50&gt;=' AS AgeRange
FROM            TblPatients AS T2 INNER JOIN
                         TblNewInPatient AS T1 ON T1.PatientNo = T2.PatientNo
WHERE        (T2.Age &gt;= 50) AND (CAST(CONVERT(VARCHAR(10), T1.CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), CAST(@DateFrom AS DATETIME), 1) 
                         AS Datetime) AND CAST(CONVERT(VARCHAR(10), CAST(@DateTo AS DATETIME), 1) AS Datetime)) AND (T1.DeleteOption = 'False')
GROUP BY T1.Diagnosis</CommandText>
