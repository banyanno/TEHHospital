                    <CommandText>SELECT T1.Order3, T1.Glasses, COUNT(T2.Female) AS F, COUNT(T2.Male) AS M, '0-4' AS AgeRange
FROM  TblVA1 AS T1 INNER JOIN
               TblPatients AS T2 ON T1.PatientNo = T2.PatientNo
WHERE (T2.Age BETWEEN 0 AND 4) AND (CAST(CONVERT(VARCHAR(10), T1.CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), 
               CAST(@DateFrom AS DATETIME), 1) AS Datetime) AND CAST(CONVERT(VARCHAR(10), CAST(@DateTO AS DATETIME), 1) AS Datetime))
GROUP BY T1.Glasses, T1.Order3
UNION
SELECT T1.Order3, T1.Glasses, COUNT(T2.Male) AS M, COUNT(T2.Female) AS F, '5-14' AS AgeRange
FROM  TblVA1 AS T1 INNER JOIN
               TblPatients AS T2 ON T1.PatientNo = T2.PatientNo
WHERE (T2.Age BETWEEN 5 AND 14) AND (CAST(CONVERT(VARCHAR(10), T1.CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), 
               CAST(@DateFrom AS DATETIME), 1) AS Datetime) AND CAST(CONVERT(VARCHAR(10), CAST(@DateTO AS DATETIME), 1) AS Datetime))
GROUP BY T1.Glasses, T1.Order3
UNION
SELECT T1.Order3, T1.Glasses, COUNT(T2.Male) AS M, COUNT(T2.Female) AS F, '15-49' AS AgeRange
FROM  TblVA1 AS T1 INNER JOIN
               TblPatients AS T2 ON T1.PatientNo = T2.PatientNo
WHERE (T2.Age BETWEEN 15 AND 49) AND (CAST(CONVERT(VARCHAR(10), T1.CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), 
               CAST(@DateFrom AS DATETIME), 1) AS Datetime) AND CAST(CONVERT(VARCHAR(10), CAST(@DateTO AS DATETIME), 1) AS Datetime))
GROUP BY T1.Glasses, T1.Order3
UNION
SELECT T1.Order3, T1.Glasses, COUNT(T2.Male) AS M, COUNT(T2.Female) AS F, '&gt;=50' AS AgeRange
FROM  TblVA1 AS T1 INNER JOIN
               TblPatients AS T2 ON T1.PatientNo = T2.PatientNo
WHERE (T2.Age &gt;= 50) AND (CAST(CONVERT(VARCHAR(10), T1.CreateDate, 1) AS DateTime) BETWEEN CAST(CONVERT(VARCHAR(10), 
               CAST(@DateFrom AS DATETIME), 1) AS Datetime) AND CAST(CONVERT(VARCHAR(10), CAST(@DateTO AS DATETIME), 1) AS Datetime))
GROUP BY T1.Glasses, T1.Order3
ORDER BY T1.Order3</CommandText>
